{
  "name": "admin-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "set TURBOPACK_ROOT=%cd% && next dev -p 3002 --turbopack",
    "dev:legacy": "next dev -p 3002",
    "build": "set TURBOPACK_ROOT=%cd% && next build --turbopack",
    "start": "next start -p 3002",
    "lint": "eslint",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@types/js-cookie": "^3.0.6",
    "axios": "^1.12.2",
    "clsx": "^2.1.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "unimported": {
    "ignorePaths": [
      "src/app/**/page.tsx",
      "src/app/**/layout.tsx",
      "src/app/**/error.tsx",
      "src/app/**/not-found.tsx",
      "src/middleware.ts",
      "tests/**",
      "__tests__/**",
      "jest.config.*",
      "next.config.js"
    ]
  }
}